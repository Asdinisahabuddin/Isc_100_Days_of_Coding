package operatorLogika;

public class day9 {
	public static void main(String[] args) {
		//NOT
		boolean a = false;
		System.out.println("NOT =" +!a);
		
		//AND
		boolean b = true;
		boolean c = false;
		boolean d = b && c;
		System.out.println(b+" AND "+c+" = " +d);
		
		//OR
		boolean e = true;
		boolean f = false;
		boolean g = e || f;
		System.out.println(e+" OR "+f+" = "+g);
	}
}g = e || f;
		System.out.println(g);
	}
}