import java.util.Scanner;
public class WhileLoopExample {
    public static void main(String[] args) {
        Scanner sc = new Scanner (System.in);
        
        System.out.print("Masukkan angka genap: ");
        int a = sc.nextInt();
        
        while (a <= 100) {
            if(a % 2 ==0){
            System.out.print(a+" ");
          
            }
            a++;
        }
    }
}