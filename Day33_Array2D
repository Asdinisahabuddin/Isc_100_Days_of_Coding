import java.util.Scanner;

public class Day33_Array2D {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		System.out.print("kolom: ");
		int a = sc.nextInt();
		
		System.out.print("baris: ");
		int b = sc.nextInt();
		
		int [][] array2D = {
			{1, 2, 3, 4},
			{5, 6, 7, 8}
		};
		
		System.out.println(array2D [a] [b]);	
	}
}