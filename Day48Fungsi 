public class Day48Fungsi {
	public static void main(String[] args) {
		double a, b;
		a = 5;
		b = hitung(a);
		System.out.println("a = "+a+", b = "+b);
		
		a = 10;
		b = hitung(a);
		System.out.println("a = "+a+", b = "+b);
		
		a = 15;
		b = hitung(a);
		System.out.println("a = "+a+", b = "+b);
	}
	public static double hitung(double input){
		double hasil = input/2;
		return hasil;
	}
}