public class day7 {
	public static void main(String[] args) {
		int b = 3;
		
		int a1 = 10;
		a1 += b;
		System.out.println(a1);
		
		int a2= 9;
		a2 -= b;
		System.out.println(a2);
		
		int a3 = 8;
		a3 *= b;
		System.out.println(a3);
		
		int a4 = 7;
		a4 /= b;
		System.out.println(a4);
		
		int a5 = 8;
		a5 %= b;
		System.out.println(a5);
	}
}