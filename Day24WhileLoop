import java.util.Scanner;
public class Day24WhileLoop {
    public static void main(String[] args) {
        Scanner sc = new Scanner (System.in);
        
        System.out.print("Masukkan angka ganjil: ");
        int a = sc.nextInt();
        
        while (a <= 100) {
            if(a % 2 !=0){
            System.out.print(a+" ");
          
            }
            a++;
        }
    }
}