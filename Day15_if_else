import java.util.Scanner;
public class Day15 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		System.out.print("Masukkan berat badan anda	: ");
		double bb = sc.nextDouble();
		
		System.out.print("Masukkan tinggi badan anda	: ");
		double tb = sc.nextDouble();
		
		double imt = bb/(tb*tb);
		System.out.println("Index Massa Tubuh mu adalah	: "+imt);
		
		if (imt < 18.5) {
			System.out.println("hati hati awas kebawa angin");
		}
		else if (imt >= 18.5 && imt < 24.9 ){
			System.out.println("bb ideal");
		}
		else{
			System.out.println("yuk bisa yuk diet");
		}
		
		
	}
}