import java.util.Scanner;
public class Day50Perkalian {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        System.out.print("masukkan n : ");
        int n = sc.nextInt();

        int hasil = 1;

        System.out.print("1");
        for (int i = 2; i <= n; i++) {
            System.out.print(" * " + i);
            hasil *= i; 
        }
        System.out.println(" = " + hasil);

    }
}